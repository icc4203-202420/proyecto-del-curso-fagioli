{
  "expo": {
    "name": "Bars App",
    "slug": "hybrid-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/beer.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/beer.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "your-app-scheme",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/beer.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/beer.png",
      "name": "Bars App",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
    
  }
}
